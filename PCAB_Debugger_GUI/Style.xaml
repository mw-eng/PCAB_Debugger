    <Style TargetType="{x:Type TextBox}" x:Key="VIEW_TEXT_MULTI">
        <Setter Property="TextWrapping" Value="Wrap" />
        <Setter Property="IsReadOnly" Value="True" />
        <Setter Property="VerticalScrollBarVisibility" Value="Auto"/>
        <Setter Property="AcceptsReturn" Value="True"/>
    </Style>
    <Style TargetType="{x:Type TextBox}" x:Key="VIEW_TEXT_LINE">
        <Setter Property="VerticalContentAlignment" Value="Center" />
        <Setter Property="TextWrapping" Value="NoWrap" />
        <Setter Property="IsReadOnly" Value="True" />
    </Style>
    <Style TargetType="{x:Type TextBox}" x:Key="EDIT_TEXT_LINE">
        <Setter Property="VerticalContentAlignment" Value="Center" />
        <Setter Property="TextWrapping" Value="NoWrap" />
        <Setter Property="IsReadOnly" Value="False" />
    </Style>
    <Style TargetType="{x:Type ComboBox}" x:Key="COMBOBOX_CONTENT_CENTER">
